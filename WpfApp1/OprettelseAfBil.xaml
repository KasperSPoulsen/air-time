        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="OprettelseAfBil" Height="450" Width="800">
    <Grid>

        <TextBlock HorizontalAlignment="Left" Margin="36,29,0,0" TextWrapping="Wrap" Text="Opret Billist" VerticalAlignment="Top" FontSize="35"/>

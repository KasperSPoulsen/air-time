<Window x:Class="WpfApp1.Konkurrence"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Konkurrence" Height="450" Width="800">
    <Grid>

        <TextBlock  HorizontalAlignment="Left" Margin="62,30,0,0" TextWrapping="Wrap" Text="Konkurrencer" VerticalAlignment="Top" Height="44" Width="184" RenderTransformOrigin="0.688,1.025" FontSize="30"/>

        <Button Content="Opret Konkurrence" HorizontalAlignment="Left" Margin="62,360,0,0" VerticalAlignment="Top" Click="OpretKonkurrence"/>
        <ListBox x:Name="KonkurrenceListBox" DisplayMemberPath="Adresse" Margin="62,100,498,199" SelectionChanged="ValgteKonkurrence"/>


    </Grid>
</Window>

<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
	</configSections>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>
	<entityFramework>
		<providers>
			<provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
		</providers>
		<contexts>
			<context type="DataAccessLayer.Context.AirTimeContext, DataAccessLayer">
				<databaseInitializer type="DataAccessLayer.Context.Initializer, DataAccessLayer" />
			</context>
		</contexts>
	</entityFramework>

	<connectionStrings>
		<add name="AirTimeContext" connectionString="Data Source=localhost\SQLEXPRESS;Initial Catalog=AirtimeDatabase;Integrated Security=SSPI;" providerName="System.Data.SqlClient" />
	</connectionStrings>
</configuration>